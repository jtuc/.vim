runtime bundle/vim-pathogen/autoload/pathogen.vim
set nocompatible
set tabstop=2
set softtabstop=2
set shiftwidth=2
set expandtab 
set autoindent
filetype plugin on
filetype indent on
syntax on
set showmatch
set lazyredraw
set mouse=a
set nohlsearch
set shortmess+=I
set splitbelow
set splitright
set ignorecase
set smartcase
set encoding=utf-8
inoremap jk <ESC>
set shell=/bin/zsh
set backspace=indent,eol,start
