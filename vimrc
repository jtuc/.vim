runtime bundle/vim-pathogen/autoload/pathogen.vim
set nocompatible
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab 
set autoindent
filetype plugin on
filetype indent on
syntax on
set showmatch
set lazyredraw
set mouse=a
set nohlsearch
set shortmess+=I
set splitbelow
set splitright
set smartcase
set encoding=utf-8
inoremap jk <ESC>
set shell=/bin/zsh
set backspace=indent,eol,start
